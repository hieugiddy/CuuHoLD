    <div id="fb-root" class=" fb_reset">
        <div style="position: absolute; top: -10000px; width: 0px; height: 0px;">
            <div></div>
        </div>
    </div>
    <script>
        window.fbAsyncInit = function() {
            FB.init({
                appId: '600859170003466',
                xfbml: true,
                version: 'v2.8'
            });
        };
