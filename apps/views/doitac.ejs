            <script type="text/javascript">
                var allowSubmit = false;
                var allowEnter = false;

                function capcha_filled() {
                    allowSubmit = true;
                }

                function capcha_expired() {
                    allowSubmit = false;
                }
                function linhVucKD(){
                    for(var i=0;i<$('.dichvu').length;i++){
                        if($('.dichvu')[i].checked)
                            return true;
                    }
                    return false;
                }
                $('#finish').click(function() {
                    allowEnter = true;
                });
                $('#myForm').on('submit', function(e) {
                    if(!linhVucKD()) {
                        e.preventDefault();
                        alert('Bạn chưa chọn lĩnh vực kinh doanh!');
                    }
                    if (allowSubmit && allowEnter) return true;
                    e.preventDefault();
                    alert('Bạn chưa xác minh Captcha!');
                });

